<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
		
    <classPathEntry    location="F:/mysql-connector-java-5.0.8-bin.jar"/>
    
    <context id="DB2Tables"    targetRuntime="MyBatis3">
    	<property name="javaFileEncoding" value="UTF-8" />
    	
        <commentGenerator >
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="false"/>
            <property name="useActualColumnNames" value="true" />
        </commentGenerator>
        
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/transport?autoReconnect=true&amp;useUnicode=true&amp;characterEncoding=utf8" 
        	userId="root" 
        	password="root">
        </jdbcConnection>
        
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        
        <javaModelGenerator targetPackage="com.transport.pojo" targetProject="transport-manage-util">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        
        <sqlMapGenerator targetPackage="com.transport.dao" targetProject="transport-manage-util">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.transport.dao" targetProject="transport-manage-util">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        
